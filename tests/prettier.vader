# vim:set et sw=4 ts=4 tw=79:
Before (Setup tests):
  :call system('cp ./_template.js ./template.js')

After (Teardown tests):
  :call delete('./template.js')

Execute:
  let bin = prettier#bin()
  AssertEqual bin, fnamemodify('./node_modules/.bin/prettier-eslint', ':p')

